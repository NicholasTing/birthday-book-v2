{
  "id": "moment",
  "label": "Moment",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Slug"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "Captured their smile"
        }
      },
      "note": {
        "type": "StructuredText",
        "config": {
          "label": "Note",
          "multi": "paragraph, strong, em, hyperlink, list-item, o-list-item"
        }
      },
      "time_label": {
        "type": "Text",
        "config": {
          "label": "Time label",
          "placeholder": "Today, yesterday, Mon"
        }
      },
      "date": {
        "type": "Timestamp",
        "config": {
          "label": "When it happened"
        }
      },
      "related_birthday": {
        "type": "Link",
        "config": {
          "label": "Related birthday",
          "select": "document",
          "customtypes": [
            "birthday"
          ]
        }
      }
    }
  }
}
